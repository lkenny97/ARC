<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.arcfinal">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/newapplogo"
        android:label="ARC System"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AscActivity"
            android:noHistory="true"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".AnswerActivity"
            android:noHistory="true"
            android:label="AskResponseClickers System">
        </activity>
        <activity android:name= ".QuestionAnsweringActivity"
            android:noHistory="true"
            android:label="ARCFinal"></activity>

        <activity android:name = ".Pop"
            android:noHistory="true"
        android:theme= "@style/AppTheme.CustomTheme"
        > </activity>

        <activity
            android:name = ".BarGraph"
            android:noHistory="true"
            android:label= "Graph Results">
        </activity>


    </application>

</manifest>
